{
  "name": "@c-ehrlich/cjode-server",
  "version": "0.9.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "bin": {
    "cjode-server": "./dist/index.js"
  },
  "main": "dist/index.js",
