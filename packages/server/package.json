    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cjode/config": "workspace:*",
    "@cjode/state": "workspace:*",
    "@types/node": "^22.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.0"
  }
